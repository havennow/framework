{
  "name": "havennow/framework",
  "description": "A set of tools built on top of laravel framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "homepage": "http://github.com/havennow",
  "support": {
    "issues": "https://github.com/havennow/framework/issues",
    "source": "https://github.com/havennow/framework"
  },
  "authors": [
    {
      "name": "Guilherme Trevisan de Oliveira",
      "email": "havennow@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "mockery/mockery": "^0.9.4",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "havennow\\": "src/havennow/"
    },
    "files": [
      "src/Ferhavennowrl/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "stub/application/app/"
    },
    "files": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
